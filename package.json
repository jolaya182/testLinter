{
  "name": "testlinter",
  "version": "1.0.0",
  "description": "This application requires nodejs and npm",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jolaya182/testLinter.git"
  },
  "scripts": {
    "test": "jest ",
    "testJest": "jest src/tests/sum --notify --config=jest.config.js",
    "testP": " node ./src/tests/puppeteer/example.js",
    "gql": "node ./src/backEnd/gql/server.js",
    "postinstall": "npm start",
    "runserver": "(cd ./src &&  nodemon  ./server)",
    "prestart": "(cd ./src && start nodemon  ./backEnd/server)  ",
    "start": " webpack serve   --hot --env PLATFORM=local --env VERSION=stag --mode development --config config/webpack.base.config.js --history-api-fallback ",
    "startwebapplication": " webpack serve  --hot --env PLATFORM=local --env VERSION=stag --mode development --config config/webpack.base.config.js --history-api-fallback --open Chrome.exe",
    "poststart": "",
    "prebuild": "webpack --mode production --config config/webpack.prod.config.js --env PLATFORM=production --env VERSION=stag --progress",
    "lint": "eslint --debug src/. ",
    "lintFix": "eslint --debug src/. --fix",
    "prettycheck": "prettier  --check src/components/App.jsx",
    "pretty": "prettier src/** --write --only-changed ",
    "prettystage": "pretty-quick --staged"
  },
  "author": "Javier O",
  "license": "MIT",
